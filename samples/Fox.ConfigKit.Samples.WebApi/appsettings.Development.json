{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "Database": {
    "ConnectionString": "Server=localhost;Database=SampleDb_Dev;Integrated Security=true;TrustServerCertificate=True",
    "CommandTimeoutSeconds": 60,
    "EnableSensitiveDataLogging": true,
    "RequireSsl": false
  },
  "ExternalApi": {
    "BaseUrl": "https://api-dev.example.com",
    "ApiKey": "dev-api-key-12345",
    "TimeoutSeconds": 60
  },
  "CustomLogging": {
    "LogDirectory": "C:\\Logs\\ConfigKitSample\\Dev"
  },
  "Security": {
    "Environment": "Development",
    "RequireHttps": false,
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:4200",
      "http://localhost:5173"
    ]
  }
}
